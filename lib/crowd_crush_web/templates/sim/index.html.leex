<iframe id="player" type="text/html" width="100%" height="100%"
  src="http://www.youtube.com/embed/<%= @video.youtubeID %>?enablejsapi=1&fs=0&modestbranding=1&loop=1&rel=0"
  frameborder="0" phx-hook='player'></iframe>

<div data-i="<%= @i %>" data-agents="<%= Jason.encode!(@agent_positions) %>" phx-hook='canvas'>
  <canvas phx-update="ignore">Canvas is not supported!</canvas>
</div>

<%= render CrowdCrushWeb.SimView, "controlbar.html" %>
